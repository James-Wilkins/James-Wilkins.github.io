{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  
    "description": "",
   
  
    "title": {
      "text": "Highest rate of Income Tax",
      "subtitle":"The top 10 countries with the highest possible rate of income tax",
      "subtitleFontStyle":"italic",
      "subtitleFontSize":10,
      "anchor": "start",
      "color": "black"
    },
    
    "data": 
      {
        "url": "https://raw.githubusercontent.com/James-Wilkins/James-Wilkins.github.io/main/IncomeTax.csv"},
  
    
    "height": 300,
    "width": 300,
    
    "mark": {
      "type": "bar",
      "color": "#b32d1c"
    },
    
    "encoding": {
    
      "x": {
        "field": "Country",
        "type": "nominal",
        "sort": "y",
        "axis": {
          "title": null,
          "grid": false,
          "ticks": false,
          "labelAngle": 45
          
        }
      },
  
      "y": {
        "field": "Income_Tax",
        "type": "quantitative",
        "title": "Income Tax(%)",
        "axis": {"grid": false}
        
      }

    }
  }
