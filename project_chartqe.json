{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.2.0.json",
  "title": {
    "text": "Central Banks, the Main Culprit?",
    "subtitle": "Comparison of Federal Funds Rate with SP500",
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 10,
    "anchor": "start",
    "color": "black"
    


  },

"height": 300,
  "width": 280,
  "data": {
    "url": "https://raw.githubusercontent.com/James-Wilkins/James-Wilkins.github.io/main/QuantitativeEasing.csv"
  },
  "layer": [
    {
      "mark": "line",
      "encoding": {
        "x": {"field": "Date", "type": "temporal"},
        "y": {"field": "SP500", "type": "quantitative", "axis": {"grid": false}}
      }
    },
    {
      "mark": {"type":"line","interpolate":"basis", "strokeWidth":0.6},
      "encoding": {
        "x": {"field": "Date", "type": "temporal"},
        "y": {
          "field": "DFF",
          "type": "quantitative",
          "axis": {"grid": false},
          "scale": {"zero": false}
        },
        "color": {"value": "green"}
      }
    }
  ],
  "resolve": {"scale": {"y": "independent"}}
}
