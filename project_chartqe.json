{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
      "title": {
    "text": "Central Banks, the Main Culprit?",
    "subtitle":". Data Source: FRED",
    "subtitleFontStyle":"italic",
    "subtitleFontSize":10,
    "anchor": "start",
    "color": "black"
  },
  

  "data": {"url": "https://raw.githubusercontent.com/James-Wilkins/James-Wilkins.github.io/main/QuantitativeEasing.csv"},
  "layer": [
    {
      "mark": "line",
      "encoding": {
        "x": {
          "field": "Date",
          "type": "temporal"
        },
        "y": {
          "field": "SP500",
          "type": "quantitative",
          "axis": {
          "grid": false
          }
        }
      }
    },
    {
      "mark": "line",
      "encoding": {
        "x": {
          "field": "Date",
          "type": "temporal"
        },
        "y": {
          "field": "DGS10",
          "type": "quantitative",
          "axis": {
            "grid": false
          },
          "scale": {"zero": false}
        },
        "color": {"value": "green"}
      }
    }
  ],
  "resolve": {"scale": {"y": "independent"}}
}
